{
    "name": "izub/laminas-google-gcm",
    "description": "OOP wrapper for Google Cloud Messaging",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "gcm",
        "push",
        "notification",
        "google"
    ],
    "require": {
        "php": "^7.4",
        "laminas/laminas-http": "^2.0",
        "laminas/laminas-json": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1.5",
        "phpstan/phpstan": "^1.2",
        "laminas/laminas-coding-standard": "~1.0.0",
        "rector/rector": "^0.15.0"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Google\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Google\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev",
            "dev-develop": "2.2.x-dev"
        }
    }
}
